<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.finance.mapper.FundYearBudgetMapper">

    <select id="getFundYearBudgetByYear" resultType="org.jeecg.modules.finance.entity.FundYearBudget">
        select * from fund_year_budget where year_num = #{yearNum} and sys_org_code = #{sysOrgCode}
    </select>
</mapper>